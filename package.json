{
  "name": "informbox",
  "private": "true",
  "description": "Тестовое задание для Информбокс",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.5.1",
    "browser-sync": "2.26.7",
    "del": "5.1.0",
    "eslint": "7.4.0",
    "eslint-config-htmlacademy": "0.5.2",
    "gulp": "4.0.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgstore": "7.0.1",
    "stylelint": "10.1.0",
    "stylelint-config-htmlacademy": "0.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss",
    "eslint": "eslint src/js/*",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp build",
    "start": "gulp start",
    "sprite": "gulp sprite"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.scss"
  ]
}
